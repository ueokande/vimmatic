{
  "name": "vimmatic",
  "description": "vimmatic",
  "version": "0.4.1",
  "scripts": {
    "start": "NODE_ENV=development nodemon --watch ./src --ext js,jsx,ts,tsx,css,html,json script/build",
    "clean": "rm -rf ./dist",
    "prebuild": "pnpm clean",
    "build": "script/build",
    "prepackage": "pnpm build",
    "package": "pnpm build && script/package",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --ext .ts,.tsx . --fix",
    "type-checks": "tsc --noEmit",
    "test": "jest",
    "test:e2e": "playwright test --browser=firefox --timeout=20000 --workers=1 --retries=3 ./e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ueokande/vimmatic.git"
  },
  "author": "Shin'ya Ueoka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ueokande/vimmatic/issues"
  },
  "homepage": "https://github.com/ueokande/vimmatic",
  "dependencies": {
    "@abraham/reflection": "^0.12.0",
    "esbuild": "^0.17.18",
    "inversify": "^6.0.1",
    "prismjs": "^1.29.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.9",
    "tslib": "^2.6.2",
    "typescript": "5.0.4",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@playwright/test": "1.26.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/chrome": "^0.0.233",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.6.0",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^17.0.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "esbuild-jest": "^0.5.0",
    "eslint": "8.49.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-standard": "^5.0.0",
    "fastify": "^4.15.0",
    "jest": "^29.6.4",
    "jszip": "^3.7.0",
    "nodemon": "^2.0.22",
    "playwright": "1.26.0",
    "playwright-webextext": "^0.0.3",
    "prettier": "2.8.8",
    "prettier-eslint": "13.0.0",
    "react-test-renderer": "17.0.2",
    "ts-node": "^10.7.0",
    "webext-agent": "^0.1.0"
  },
  "engines": {
    "node": "20.1.0"
  },
  "packageManager": "pnpm@8.7.4"
}
